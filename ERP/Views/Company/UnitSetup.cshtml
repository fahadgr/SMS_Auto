
@{
    ViewBag.Title = "UnitSetup";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<div class="row">
    <div class="col-lg-12">
        <div class="white-box">
            <div class="">
                <div class="row">
                    <div class="floating-labels has-feedback">

                        <div class="col-lg-12 col-md-12 col-sm-12">

                            <input type="hidden" name="CompanyCode" value="" />

                            <div class="form-group col-md-8 info m-b-20">
                                <input type="text" class="form-control  input-sm" id="CompanyDesc" name="CompanyDesc" maxlength="40" style="text-transform:uppercase;font-size:12px" value="" required><span class="highlight"></span> <span class="bar"></span><span class="bar"></span>
                                <label for="input9">Unit Description</label>
                            </div>

                            <div class="form-group col-md-4 m-b-20">
                                <input type="text" class="form-control input-sm" id="Address1" maxlength="100" name="Address1" style="text-transform:uppercase;font-size:12px" value="" required><span class="highlight"></span> <span class="bar"></span><span class="bar"></span>
                                <label for="input9">Name Abbreviate</label>
                            </div>
                            <div class="form-group col-md-12 m-b-20">
                                <input type="text" class="form-control input-sm" id="Address2" maxlength="100" name="Address2" style="text-transform:uppercase;font-size:12px" value="" required><span class="highlight"></span> <span class="bar"></span><span class="bar"></span>
                                <label for="input9">Address</label>
                            </div>

                            <div class="form-group col-md-4 m-b-20">
                                <input type="tel" class="form-control input-sm" id="Phone1" name="Phone1" maxlength="12" style="font-size: 12px" value="" required><span class="highlight"></span> <span class="bar"></span><span class="bar"></span>
                                <label for="input9">City</label>
                            </div>
                            <div class="form-group col-md-4 m-b-20">
                                <input type="tel" class="form-control input-sm" id="Phone2" name="Phone2" maxlength="12" style="font-size: 12px" value="" required><span class="highlight"></span> <span class="bar"></span><span class="bar"></span>
                                <label for="input9">Phone</label>
                            </div>
                            <div class="form-group row col-md-12">
                                <div class="offset-md-4 col-md-4" style="margin-top:10px">
                                    <button type="submit" class="btn btn-info btn-rounded" style="width:500px"> Add </button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

